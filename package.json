  "scripts": {
    "build": "rm -rf dist && tsc",
    "dev": "tsx watch src/index.ts",
    "test": "NODE_ENV=test vitest run",
    "test:watch": "NODE_ENV=test vitest --watch",
    "test:k6": "NODE_ENV=test k6 run tests/integration-test/rinha.js",
    "test:debug": "vitest --inspect-brk --no-file-parallelism",
    "test:debug-run": "vitest run --inspect-brk --no-file-parallelism",
    "start:processors": "docker compose --file vendor/docker-compose.yml up --detach",
    "start:monitoring": "docker compose --file compose.monitoring.yaml up --detach",
    "stop:processors": "docker compose --file vendor/docker-compose.yml down",
    "stop:monitoring": "docker compose --file compose.monitoring.yaml down"
  },
  "keywords": [
    "Hono",
    "BullMQ",
    "Redis",
    "Payment",
    "Docker",
    "OpenTelemetry",
    "Monitoring",
    "TypeScript",
    "Vitest",
    "Winston",
    "Zod"
  ],
  "dependencies": {
    "@bull-board/api": "^6.12.0",
    "@bull-board/hono": "^6.12.0",
    "@bull-board/ui": "^6.11.0",
    "@hono/node-server": "^1.18.0",
    "@hono/zod-openapi": "^0.19.9",
    "@opentelemetry/auto-instrumentations-node": "^0.62.0",
    "@opentelemetry/exporter-prometheus": "^0.203.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.203.0",
    "@opentelemetry/instrumentation-http": "^0.203.0",
    "@opentelemetry/sdk-metrics": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.203.0",
    "bullmq": "^5.56.9",
    "bullmq-otel": "^1.0.1",
    "dotenv": "^17.2.1",
    "hone": "^1.1.0",
    "ioredis": "^5.7.0",
    "winston": "^3.17.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@types/node": "^24.1.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "vitest-fetch-mock": "^0.4.5"
  }
